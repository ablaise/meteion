{
    "name": "ablaise/meteion",
    "description": "Meteion is a straightforward PHP tool that loads FFXIV client data into a relational database.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 || ^8.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "symfony/serializer": "^5.4",
        "symfony/property-access": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix lib --verbose --rules=@Symfony"
    },
    "autoload": {
        "psr-4": { "Meteion\\": "lib/Meteion" }
    }
}
